(def dec (fn [a] (- a 1)))
(def inc (fn [a] (+ a 1)))
(def not (fn [a] (if a false true)))

(def neg (fn [a] (- 0 a)))

(def nil? (fn [a] (= (type a) "nil")))

(def include (fn [f]
              (do 
               (eval (read (slurp f)))
               (println (str "Loaded " f)))))

(def second (fn [a] (first (rest a))))

(def newline (fn[] (puts \newline)))

    (def println (fn [a] (do 
                          (puts a)
                          (newline))))

